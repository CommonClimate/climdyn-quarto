@article{Zhu:GRL2020,
	annote = {doi: 10.1029/2019GL086908},
	author = {Zhu, Feng and Emile-Geay, Julien and Hakim, Gregory J. and King, Jonathan and Anchukaitis, Kevin J.},
	booktitle = {Geophysical Research Letters},
	da = {2020/04/28},
